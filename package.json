{
  "name": "@victorpavn/library-throttle",
  "version": "0.0.0",
  "description": "Small library for handling throttle calls",
  "keywords": [
    "typescript",
    "throttle"
  ],
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "tsc",
    "test:dev": "vitest",
    "test:dev:debug": "vitest --silent=false",
    "test": "vitest run",
    "preview": "vite preview",
    "pre-commit": "pnpm run lint && pnpm run test",
    "playground": "pnpm ts-node src/playground.ts",
    "playground:watch": "pnpm ts-node-dev --respawn src/playground.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/node": "^22.8.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.6.2",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PavelescuVictor/library-throttle.git"
  },
  "author": "Victor Pavelescu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PavelescuVictor/library-throttle/issues"
  },
  "homepage": "https://github.com/PavelescuVictor/library-throttle#readme"
}
